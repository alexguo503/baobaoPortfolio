{
  "fullScreen": {
    "enable": true,
    "zIndex": 1
},
  "fps_limit":120,
  "interactivity":{
    "detectsOn":"window",
    "events":{
      "onClick":{
        "enable":true,
        "mode":"repulse"
      },
      "onHover":{
        "enable":false,
        "mode":"bubble"
      },
      "resize":true
    },
    "modes":{
      "push":{
        "particles_nb":4
      },
      "repulse":{
        "distance":400,
        "duration":0.1
      }
    }
  },
  "particles":{
    "color":{
      "value":"#4361ee"
    },
    "links":{
      "color":"#ffffff",
      "distance":100,
      "enable":true,
      "opacity":0.9,
      "width":1
    },
    "move":{
      "bounce":false,
      "direction":"none",
      "enable":true,
      "outMode":"out",
      "random":true,
      "speed":2.5,
      "straight":false
    },
    "number":{
      "density":{
        "enable":true,
        "area":5000
      },
      "value":100
    },
    "opacity":{
      "value":1
    },
    "shape":{
      "type":"circle",
      "stroke":{
          "width": 0.5,
          "color": "#000000"
      },
      "polygon": {
          "nb_sides": 7
      }
      
    },
    "size":{
      "random":true,
      "value":3
      
    }
  },
  "detectRetina":true
}